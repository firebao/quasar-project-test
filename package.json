{
  "name": "quasar-project-test",
  "version": "0.0.1",
  "description": "A Quasar Project",
  "productName": "Quasar App",
  "author": "wwj <318348750@qq.com>",
  "private": true,
  "type": "module",
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "dev": "quasar dev",
    "build": "quasar build",
    "test:unit:ui": "vitest --ui",
    "test:unit": "vitest",
    "test:unit:ci": "vitest run",
    "test:e2e": "cross-env NODE_ENV=test start-test \"quasar dev\" http-get://127.0.0.1:8080 \"cypress open --e2e\"",
    "test:e2e:ci": "cross-env NODE_ENV=test start-test \"quasar dev\" http-get://127.0.0.1:8080 \"cypress run --e2e\"",
    "test:component": "cross-env NODE_ENV=test cypress open --component",
    "test:component:ci": "cross-env NODE_ENV=test cypress run --component",
    "prepare": "husky install",
    "commit": "git-cz"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.4",
    "@turf/turf": "^6.5.0",
    "axios": "^1.2.1",
    "lodash": "^4.17.21",
    "lowdb": "^7.0.1",
    "mars3d": "^3.7.9",
    "mars3d-cesium": "^1.116.1",
    "mars3d-space": "^3.7.9",
    "pinia": "^2.0.11",
    "quasar": "^2.8.0",
    "three": "^0.164.1",
    "vue": "^3.2.29",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/cz-commitlint": "^19.2.0",
    "@intlify/vite-plugin-vue-i18n": "^3.3.1",
    "@pinia/testing": "^0.1.3",
    "@quasar/app-vite": "^1.4.3",
    "@quasar/quasar-app-extension-testing": "^2.2.0",
    "@quasar/quasar-app-extension-testing-e2e-cypress": "^6.0.0",
    "@quasar/quasar-app-extension-testing-unit-vitest": "^1.0.0",
    "@types/lodash": "^4.17.0",
    "@types/lowdb": "^1.0.15",
    "@types/node": "^20.11.30",
    "@types/three": "^0.164.0",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "@vitest/coverage-v8": "^1.4.0",
    "@vitest/ui": "^1.3.1",
    "@vue/test-utils": "^2.4.4",
    "autoprefixer": "^10.4.2",
    "cypress": "^13.6.6",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-vue": "^9.0.0",
    "happy-dom": "^14.3.9",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^2.5.1",
    "stylelint": "^16.4.0",
    "stylelint-config-recommended-scss": "^14.0.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-scss": "^6.2.1",
    "typescript": "^4.5.4",
    "vite-plugin-mars3d": "^4.0.0",
    "vite-plugin-style-import": "^2.0.0",
    "vitest": "^1.3.1"
  },
  "engines": {
    "node": "^20 || ^18 || ^16",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  },
  "lint-staged": {
    "*.{js,vue,ts}": "eslint --cache --fix",
    "*.--write": "prettier --write"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  }
}
